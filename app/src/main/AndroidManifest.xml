<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:icon="@drawable/logo_agroboost"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_agroboost"
        android:supportsRtl="true"
        android:name=".MyApplication"
        android:theme="@style/Theme.NavigationDrawawerKotlin"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".MainActivity"

            tools:targetApi="n">
        </activity>

        <activity android:name=".Login"
            android:resizeableActivity="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register">

        </activity>
        <activity android:name=".Forgot">

        </activity>
        <activity android:name=".NewPassword">

        </activity>
        <activity android:name=".Verificacion">

        </activity>
        <activity android:name=".Inicio"
            android:theme="@style/Theme.Inicio">
            <meta-data
                android:name="android.app.lib_name"
                android:value=""/>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>